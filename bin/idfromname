#!/bin/bash


IP=$(aws --profile readonly ec2 describe-instances --query "Reservations[].Instances[].InstanceId" --filter "Name=tag:Name,Values=$1" --output text)
if [ "$IP" == "" ]; then
  
 IP=$(aws --profile prod-readonly ec2 describe-instances --query "Reservations[].Instances[].InstanceId" --filter "Name=tag:Name,Values=$1" --output text)

fi

echo $IP
